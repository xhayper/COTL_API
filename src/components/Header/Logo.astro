---
import { Image } from "@astrojs/image/components";

type Props = {
    size: number;
};
const { size } = Astro.props as Props;
---

<Image
    class={"logo"}
    src={"/icon.svg"}
    alt={"Logo"}
    quality={100}
    width={size}
    height={size}
    format={"png"}
    loading={"eager"}
    style={{
        "object-fit": "scale-down",
        "aspect-ratio": "1",
        "min-height": size,
        "min-width": size
    }}
/>
